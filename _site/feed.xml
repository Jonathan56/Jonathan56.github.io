// a parser for the date
var parseDate = d3.time.format(&quot;%Y-%m-%d %H:%M:%S&quot;).parse;
var colorNetload = d3.scale.category20();
var colorLoad = d3.scale.category20();
var colorGen = d3.scale.category20();

// a scale for the time
